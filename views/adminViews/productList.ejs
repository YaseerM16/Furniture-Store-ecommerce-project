            </div>
            <div class="row">
                <div class="col-12">
                    <div class="table-responsive cart_table">
                        <table>
                            <thead>
                                <tr>
                                    <th scope="col">Product Name</th>
                                    <th scope="col">Product Category</th>
                                    <th scope="col">Product Image</th>

                                    <th scope="col">Price in INR</th>
                                    <th scope="col">Instock</th>
                                    <th scope="col">List/Unlist</th>
                                    <th scope="col">Actions</th>
                                </tr>
                            </thead>
                            <tbody>


                                <% for(let i=0;i<productDet.length;i++){ %>
                                    <tr>
                                        <th scope="row">
                                            <%= productDet[i].productName %>
                                        </th>
                                        <td>
                                            <%= productDet[i].parentCategory.categoryName%>
                                        </td>


                                        <td>
                                            <% for(let j=0;j<productDet[i].productImage.length;j++){ %>
                                                <img src="assets/uploadimages/uploads/<%=productDet[i].productImage[j]%>"
                                                    style="height: 60px; width: 60px; border-radius: 0;">
                                                <% } %>
                                        </td>

                                        <td>
                                            <%= productDet[i].productPrice %>
                                        </td>
                                        <td>
                                            <%= productDet[i].productStock %>
                                        </td>
                                        <% if(productDet[i].isListed){ %>
                                            <td><button id="unblock<%i%>" onclick="fnBlock('<%=productDet[i]._id %>')"
                                                    type="button" class="btn btn-danger">Block</button> </td>
                                            <% }else{ %>
                                                <td><button id="block<%i%>"
                                                        onclick="fnUnBlock('<%=productDet[i]._id %>')" type="button"
                                                        class="btn btn-success">Unblock</button> </td>
                                                <% } %>
                                                    <td>
                                                        <a
                                                            href="/editProduct?pid=<%= productDet[i]._id %>&cid=<%= productDet[i].parentCategory._id %>">

                                                            <button id="unblock<%i%>" data-target="#categoryModal<%=i%>"
                                                                data-toggle="modal" type="button"
                                                                class="btn btn-info">Edit</button>

                                                        </a>

                                                    </td>
                                    </tr>
                                    <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>





    <script>
        async function fnBlock(productId) {
            let response = await fetch(`/blockProduct?id=${productId}`, {
                method: 'GET',
            })
            let result = await response.json()
            if (result.block) {
                await Swal.fire({
                    icon: "error",
                    title: "Product Blocked",
                });
            }
            window.location.reload();
        }

        async function fnUnBlock(productId) {
            let response = await fetch(`/unBlockProduct?id=${productId}`, {
                method: 'GET',
            })
            let result = await response.json()
            if (result.unBlock) {
                await Swal.fire({
                    icon: "success",
                    title: "Product Unblocked",
                    confirmButtonText: "OK",
                });
            }
            window.location.reload();
        }

    </script>
