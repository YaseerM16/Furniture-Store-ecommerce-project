        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script>
        // $("#addCouponForm").validate();
        $(document).ready(function () {
            $("#addCouponForm").validate({
                submitHandler: async function (form, event) {
                    try {
                        event.preventDefault()
                        let formData = {
                            couponCode: form.couponCode.value,
                            discountPercentage: form.discountPercentage.value,
                            startDate: form.startDate.value,
                            expiryDate: form.expiryDate.value,
                            minimumPurchase: form.minimumPurchase.value,
                            maximumDiscount: form.maximumDiscount.value
                        }

                        let response = await fetch('/admin/couponManagement/addCoupon', {
                            method: 'POST',
                            headers: { "Content-Type": "application/json" },
                            body: JSON.stringify(formData)
                        });

                        let result = await response.json();
                        console.log(result);

                        if (result.couponAdded) {
                            Swal.fire({
                                icon: "success",
                                title: "Your new coupon has been added",
                                showConfirmButton: true,
                            }).then((result) => {
                                if (result.isConfirmed) {
                                    window.location.reload() // Reload the page after user confirms
                                }
                            });
                        } else if (result.couponCodeExists) {
                            Swal.fire({
                                icon: "error",
                                title: "Coupon code already exists",
                                text: "Failed to add"
                            }).then((result) => {
                                if (result.isConfirmed) {
                                    window.location.reload() // Reload the page after user confirms
                                }
                            });
                        }
                    } catch (error) {
                        console.log("Error while sending the fetch to add the coupon " + error);
                    }


                }
            })

        })

    </script>
