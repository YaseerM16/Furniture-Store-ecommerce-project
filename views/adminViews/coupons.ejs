                        <table class="table">
                            <thead class="thead-dark">
                                <tr>
                                    <th scope="col">S No</th>
                                    <th scope="col">Coupon Code</th>
                                    <th scope="col">Discount Percentage</th>
                                    <th scope="col">Start Date</th>
                                    <th scope="col">Expiry Date</th>
                                    <th scope="col">Minimum Purchase</th>
                                    <th scope="col">Maximum Discount</th>
                                    <th scope="col">Current Status</th>
                                    <th scope="col">Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for(let i=0;i<coupons.length;i++){ %>
                                    <tr>
                                        <td>
                                            <%= i + 1 %>
                                        </td>
                                        <th scope="row">
                                            <%= coupons[i].couponCode %>
                                        </th>
                                        <td>
                                            <%= coupons[i].discountPercentage %>
                                        </td>
                                        <td>
                                            <%= coupons[i].startDate.toLocaleDateString('en-US', { year: 'numeric' ,
                                                month: '2-digit' , day: '2-digit' }) %>
                                        </td>
                                        <td>
                                            <%= coupons[i].expiryDate.toLocaleDateString('en-US', { year: 'numeric' ,
                                                month: '2-digit' , day: '2-digit' }) %>
                                        </td>
                                        <td>
                                            <%= coupons[i].minimumPurchase %>
                                        </td>
                                        <td>
                                            <%= coupons[i].maximumDiscount %>
                                        </td>
                                        <td>
                                            <%= coupons[i].currentStatus %>
                                        </td>
                                        <td>
                                            <button type="button" class="btn btn-info" data-bs-toggle="modal"
                                                data-bs-target="#editModal<%= i %>">
                                                <i class="fas fa-edit"></i>
                                            </button>
                                        </td>
                                    </tr>
                                    <!--Edit Modal -->

                                    <div class="modal fade" id="editModal<%= i %>" tabindex="-1"
                                        aria-labelledby="exampleModalLabel" aria-hidden="true">
                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="exampleModalLabel">Edit Coupon</h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal"
                                                        aria-label="Close"></button>
                                                </div>
                                                <form id="editCouponForm<%= i %>">
                                                    <div class="modal-body">

                                                        <div class="couponCodeInput p-2">
                                                            <label for="couponCodeEdit<%= i %>" class="mb-2">Coupon
                                                                Code: </label>
                                                            <div class="input-group">
                                                                <input id="couponCodeEdit<%= i %>" name="couponCode"
                                                                    type="text" class="form-control"
                                                                    value="<%= coupons[i].couponCode %>" required
                                                                    minlength="5" maxlength="16">
                                                            </div>
                                                            <div class="form-text">Add a relevant coupon code eg:
                                                                NEWYEAR2024, PONGAL2024, VYZ548</div>
                                                        </div>

                                                        <div class="discountPercentageInput p-2">
                                                            <label for="discountPercentageEdit<%= i %>"
                                                                class="mb-2">Discount Percentage: </label>
                                                            <div class="input-group">
                                                                <input id="discountPercentageEdit<%= i %>"
                                                                    name="discountPercentage" type="text"
                                                                    class="form-control"
                                                                    value="<%= coupons[i].discountPercentage %>"
                                                                    required>
                                                            </div>
                                                            <div class="form-text">Choose a percentage to be discounted.
                                                                Between 5% and 90%.</div>
                                                        </div>

                                                        <div class="startDateInput p-2">
                                                            <label for="startDate<%= i %>">Coupon Start
                                                                Date:</label>
                                                            <div class="input-group">
                                                                <input name="startDate" id="startDate<%= i %>"
                                                                    type="date" class="form-control"
                                                                    value="<%= coupons[i].startDate.toISOString().substring(0, 10) %>"
                                                                    required>
                                                            </div>
                                                            <div class="form-text">Choose a start date for the discount
                                                                to be considered.</div>
                                                        </div>
                                                        <div class="expiryDateInput p-2">
                                                            <label for="expiryDate<%= i %>">Coupon Expiry Date :
                                                            </label>
                                                            <div class="input-group">
                                                                <input id="expiryDate<%= i %>" name="expiryDate"
                                                                    type="date" class="form-control"
                                                                    value="<%= coupons[i].expiryDate.toISOString().substring(0, 10) %>"
                                                                    required>
                                                            </div>
                                                            <div class="form-text">Choose an exipry date for the
                                                                discount to end.</div>
                                                        </div>

                                                        <div class="minimumPurchaseInput p-2">
                                                            <label for="minimumPurchaseEdit<%= i %>">Minimum
                                                                Purchase: </label>
                                                            <div class="input-group">
                                                                <input id="minimumPurchaseEdit<%= i %>"
                                                                    name="minimumPurchase" type="text"
                                                                    class="form-control"
                                                                    placeholder="Enter minimum purchase required"
                                                                    value="<%= coupons[i].minimumPurchase %>" required>
                                                            </div>
                                                            <div class="form-text">Minimum amount of purchase to be done
                                                                to allow the coupon discount.
                                                            </div>
                                                        </div>

                                                        <div class="maximumDiscount p-2">
                                                            <label for="maximumDiscountEdit<%= i %>">Maximum
                                                                Discount: </label>
                                                            <div class="input-group">
                                                                <input id="maximumDiscountEdit<%= i %>"
                                                                    name="maximumDiscount" type="text"
                                                                    class="form-control"
                                                                    placeholder="Maximum discount applicable"
                                                                    value="<%= coupons[i].maximumDiscount %>" required>
                                                            </div>
                                                            <div class="form-text">Maximum amount of discount to be
                                                                allowed while using the coupon.
                                                            </div>
                                                        </div>

                                                    </div>
                                                    <div class="modal-footer">
                                                        <button type="button" class="btn btn-secondary"
                                                            data-bs-dismiss="modal">Close</button>
                                                        <button type="submit" class="btn btn-info">Save changes</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- <script>
                                        $('#editCouponForm<%= i %>').validate()
                                        console.log($('#editCouponForm<% =i %>'));
                                    </script> -->
                                    <% } %>

                            </tbody>
                        </table>
                    </section>
                </div>
            </div>
        </div>
    </section>
