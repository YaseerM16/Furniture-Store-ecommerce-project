                                        <option disabled selected>Select Parent Category</option>
                                        <% for(let i=0;i<categoryDet.length;i++){ %>
                                            <option value="<%=categoryDet[i]._id%>">
                                                <%=categoryDet[i].categoryName%>
                                            </option>
                                            <% } %>
                                    </select>
