                                <label for="productPrice" class="my-2">Product Price</label>
                                <div class="d-flex justify-content-center">
                                    <input class="form-control" name="productPrice" type="text" id="productPrice"
                                        placeholder="Enter product price" maxlength="50" style="max-width: 250px;">
                                </div>
                            </div>
                            <br>
                            <div class="form-group text-center">
                                <label for="productStock" class="my-2">Product Stock</label>
                                <div class="d-flex justify-content-center">
                                    <input class="form-control" name="productStock" type="text" id="productStock"
                                        placeholder="Enter product stock" maxlength="50" style="max-width: 250px;">
                                </div>
                            </div>
                            <br>
