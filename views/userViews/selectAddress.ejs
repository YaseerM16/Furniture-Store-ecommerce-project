        const addresses = '<%- JSON.stringify(addresses) %>'

        const submitAddressBtn = document.querySelector('#submitAddressBtn');
        submitAddressBtn.addEventListener('click', () => {
            const selectedAddressRadio = document.querySelector('input[name="selectedAddress"]:checked');
            if (selectedAddressRadio) {
                const selectedAddressId = selectedAddressRadio.value;
                const url = `/RedirectPaymentPage?addressId=${selectedAddressId}`;

                fetch(url, {
                    method: 'GET'
                })
                    .then(response => response.json())
                    .then(data => {
                        // Handle response data
                        window.location.href = `/payMethodPage`;
                        console.log("Address data sended");
                    })
                    .catch(error => {
                        // Handle error
                        console.log("Error During the Fetch in the send data to select payment page  " + error);
                    });
            } else {
                const selectAdd = new bootstrap.Modal(
                    document.getElementById("popupMessageModal")
                );
                selectAdd.show();
                document
                    .getElementById("ok-btn")
                    .addEventListener("click", () => {
                        selectAdd.hide();
                    });
                // Show success message or redirect to a new page
                console.log("SelectAdress modal shown");
            }


        });

        const editAddressBtns = document.querySelectorAll(".edit-buttons");
        const editAddressModal = new bootstrap.Modal(document.getElementById("editAddressModal"));
        const deleteAddressModal = new bootstrap.Modal(document.getElementById('deleteAddressModal'));
        const editAddressForm = document.getElementById("edit-address");
        const deleteAddressBtns = document.querySelectorAll("#delete-address-btn")

        document.querySelector(".cancel-button").addEventListener("click", () => {
            editAddressModal.hide();
            window.location.reload()
        })

        editAddressBtns.forEach(btn => {
            btn.addEventListener("click", (event) => {
                const addressId = event.target.getAttribute('data-address-id');
                // console.log(event);
                // console.log("addressId:", addressId);
                const address = JSON.parse(addresses).find(address => address._id == addressId);
                if (address) {
                    // console.log("address", address);
                    // editAddressForm.address_id.value = addressId;
                    document.getElementById('address_id').value = addressId;
                    document.getElementById('edit_address_title').value = address.addressTitle;
                    document.getElementById('edit_address_line1').value = address.addressLine1;
                    document.getElementById('edit_address_line2').value = address.addressLine2;
                    document.getElementById('edit_address_city').value = address.city;
                    document.getElementById('edit_address_state').value = address.state;
                    document.getElementById('edit_address_zip').value = address.zipcode;
                    document.getElementById('edit_address_firstName').value = address.firstName;
                    document.getElementById('edit_address_lastName').value = address.lastName;
                    document.getElementById('edit_address_phone').value = address.phone;
                    document.getElementById('edit_address_alternate').value = address.alternateNumber;
                    // Populate other fields similarly
                    editAddressModal.show();
                }
            });
        });

        editAddressForm.addEventListener("submit", (event) => {
            event.preventDefault();
            console.log("value : ", editAddressForm.address_id);
            const formData = new FormData(editAddressForm);
            const data = Object.fromEntries(formData.entries());
            const urlEncodedData = new URLSearchParams(data);
            const addressId = editAddressForm.address_id.value;

            // Send HTTP request to the server
            fetch(`/editAddress?id=${addressId}`, {
                method: "POST",
                headers: {
                    "Content-Type": "application/x-www-form-urlencoded",
                },
                body: urlEncodedData,
            })
                .then(response => {
                    if (response.ok) {
                        const successModal = new bootstrap.Modal(
                            document.getElementById("successModal")
                        );
                        successModal.show();
                        document
                            .getElementById("okButton")
                            .addEventListener("click", () => {
                                window.location.href = "/checkOutAddress";
                            });
                        // Show success message or redirect to a new page
                        console.log("Address updated successfully");
                    } else {
                        // Show error message
                        console.error("Failed to update address");
                    }
                    // Handle the edit request here
                    editAddressModal.hide();
                })
                .catch(error => {
                    console.error("Error:", error);
                });
        });

        // delete address

        deleteAddressBtns.forEach(btn => {
            btn.addEventListener("click", () => {
                deleteAddressModal.show();
            })
        })




        document.getElementById('confirm-delete-address').addEventListener('click', async () => {
            const addressId = document.getElementById('delete-address-btn').dataset.addressId;
            const response = await fetch(`/deleteAddress?id=${addressId}`, {
                method: 'DELETE',
            });

            if (response.ok) {
                const result = await response.json();
                if (result.success) {
                    // show success message in modal
                    deleteAddressModal.hide();
                    // document.getElementById('delete-modal-body').innerHTML = 'Address deleted successfully.';
                    const deleteSuccessModal = new bootstrap.Modal(document.getElementById('deleteSuccessModal'));
                    deleteSuccessModal.show();
                    document
                        .getElementById("delOkButton")
                        .addEventListener("click", () => {
                            window.location.href = "/checkOutAddress";
                        });
                } else {
                    // show error message in modal
                    const deleteAddressModal = new bootstrap.Modal(document.getElementById('deleteAddressModal'));
                    deleteAddressModal.hide();
                    const errorModal = new bootstrap.Modal(document.getElementById('errorModal'));
                    errorModal.show();
                    document.getElementById('error-message').textContent = 'Error deleting address.';
                }
            } else {
                // show error message in modal
                const deleteAddressModal = new bootstrap.Modal(document.getElementById('deleteAddressModal'));
                deleteAddressModal.hide();
                const errorModal = new bootstrap.Modal(document.getElementById('errorModal'));
                errorModal.show();
                document.getElementById('error-message').textContent = 'Error deleting address.';
            }
        });

