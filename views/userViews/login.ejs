                        </div>
                        <div class="box_inner">
                            <h3 class="title">Log in to your account</h3>
                            <form id="myForm" action="/logIn" method="POST">
                                <% if(invalid){ %>
                                    <div class="m-3 py-1 rounded text-center border bg-danger-subtle">
                                        <h3 class="text-danger">Invalid Credentials!!</h3>
                                    </div>
                                    <% } %>
                                        <div class="row">
                                            <div class="col-12">
                                                <div class="form-group">
                                                    <label>Email/username</label>
                                                    <input type="email" name="email" style="font-size: 22px;"
                                                        class="form-control form-control-custom" placeholder="Email I'd"
                                                        autocomplete="off" required>
                                                    <% if (errors && errors.length> 0) { %>
                                                        <% errors.forEach(error=> { %>
                                                            <% if (error.path==='email' ) { %>
                                                                <div class="alert alert-danger">
                                                                    <%= error.msg %>
                                                                </div>
                                                                <% } %>
                                                                    <% }); %>
                                                                        <% } %>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <div class="form-group">
                                                    <label>Password <a href="/forget-password"
                                                            class="thm-color-one">Forgot
                                                            Password?</a></label>
                                                    <input type="password" name="password" style="font-size: 22px;"
                                                        class="form-control form-control-custom" id="password_value"
                                                        placeholder="password" autocomplete="off" required>
                                                    <% if (errors && errors.length> 0) { %>
                                                        <% errors.forEach(error=> { %>
                                                            <% if (error.path==='password' ) { %>
                                                                <div class="alert alert-danger">
                                                                    <%= error.msg %>
                                                                </div>
                                                                <% } %>
                                                                    <% }); %>
                                                                        <% } %>
                                                                            <button id="password_eye" class="fal fa-eye"
                                                                                type="button">
                                                                                <span></span>
                                                                            </button>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <div class="form-group">
                                                    <div class="custom-control custom-radio me-sm-2 form-check">
                                                        <input type="radio"
                                                            class="custom-control-input form-check-input"
                                                            id="radioValidation">
                                                        <label class="custom-control-label" for="radioValidation">Keep
                                                            me logged
                                                            in</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-12">
                                                <button type="submit"
                                                    class="thm-btn btn-rectangle thm-bg-color-one w-100">Login</button>
                                                <p class="mt-3 mb-0 text-center fw-500">Don't have an account? <a
                                                        href="/signUp" class="thm-color-one">Register</a></p>
                                            </div>
                                        </div>
                            </form>
                            <a id="googleSignInButton" href="/auth/google" class="google-signin-button">
                                <div class="google-icon-container" style="display: flex; justify-content: center;">
                                    <svg class="google-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48"
                                        fill="#fff">
                                        <path fill="#FFC107"
                                            d="M46.71,20.69H46v-4h-2v4H4v-4H2v4H1.29C0.59,20.69,0,21.28,0,21.98v4.04c0,0.7,0.59,1.29,1.29,1.29h2v4 h2v-4h2v4h36v-4h2v4h2.71c0.7,0,1.29-0.59,1.29-1.29v-4.04C48,21.28,47.41,20.69,46.71,20.69z">
                                        </path>
                                        <path fill="#FF3D00"
