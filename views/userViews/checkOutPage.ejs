                    async function couponFunc(couponId) {
                        try {
                            const response = await fetch(`/applyCoupon?couponID=${couponId}&grandTotal=${parsedCartTot}`, {
                                method: 'GET',
                            });
                            const result = await response.json();

                            if (result.couponCofirmed) {
                                const appliedDiscount = document.querySelector('.appliedDisCount')
                                const discAmt = document.getElementById('discountAmt')
                                if (appliedDiscount) {
                                    appliedDiscount.classList.remove('d-none');
                                } else {
                                    console.log("Element with class 'appliedDisCount' not found");
                                }
                                discAmt.innerHTML = `<b>₹&nbsp;${result.appliedDisCount.toFixed(2)}</b>`;
                                Swal.fire({
                                    icon: "success",
                                    title: "Your coupon has been Applied",
                                    showConfirmButton: true,
                                }).then((result) => {
                                    if (result.isConfirmed) {
                                        window.location.reload() // Reload the page after user confirms
                                    }
                                })
                            } else if (result.couponCodeExists) {
                                await Swal.fire({
                                    icon: "error",
                                    title: "Coupon Code Applied Already",
                                    text: "Coupon Code must be Added Once"
                                });
                            }
                        } catch (error) {
                            console.log("Error while sending the apply coupon request :" + error);
                        }
                    }
